TOKEN "read" READ
TOKEN "active_app_endpoint_read_8658" READ

NODE active_app_0
SQL >
    %
    select round(sum(duration) / 60 / 60, 4) duration, if(app == 'Electron', 'VSCode', app) app
    from events
    where user = 'alrocar' and app is not null and toDate(timestamp) = today() and status = 'unlocked'
    group by app
    order by duration desc

NODE active_app_1
SQL >
    SELECT groupArray(duration) data, groupArray(app) categories FROM active_app_0
